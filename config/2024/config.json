          {
            "code": "nf",
            "title": "Notes Fed",
            "type": "counter",
            "defaultValue": 0,
            "min": 0,
            "required": false
          },
          
          {
            "code": "amn",
            "title": "Missed note",
            "type": "counter",
            "defaultValue": 0,
            "min": 0,
            "required": false
          },
          {
