            "defaultValue": 0,
            "min": 0,
            "required": false
          },
          {
            "code": "ausc",
            "title": "Speaker Scored",
            "type": "counter",
            "defaultValue": 0,
            "min": 0,
            "required": false
          },
          {
            "code": "miss",
            "title": "Missed note",
            "type": "counter",
            "defaultValue": 0,
            "min": 0,
            "required": false
          }
