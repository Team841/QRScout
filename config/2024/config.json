            "type": "counter",
            "defaultValue": 0,
            "min": 0,
            "required": false
          },
          {
            "code": "tsc",
            "title": "Speaker Scored",
            "type": "counter",
            "defaultValue": 0,
            "min": 0,
            "required": false
          },
          {
            "code": "tascp",
            "title": "Amplified Speaker",
            "type": "counter",
            "defaultValue": 0,
            "min": 0,
            "required": false
