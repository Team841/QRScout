          },
          {
            "code": "GR",
            "title": "Red/Green Flag",
            "type": "select",
            "defaultValue": "No Flag",
            "required": false,
            "choices": {
              "No FLag": "No Flag",
              "Green Flag": "Green Flag",
              "Red Flag": "Red Flag"
            }
          },
          {
            "code": "co",
            "title": "Comments",
            "type": "text",
            "min": 0,
            "max": 750,
            "required": false
          }
        ]
      }
    ]
